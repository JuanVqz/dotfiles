filetype plugin indent on
syntax on

""let &t_8f="\<Esc>[38;2;%lu;%lu;%lum"
""let &t_8b="\<Esc>[48;2;%lu;%lu;%lum"
""set termguicolors
set background=dark
colorscheme solarized
set relativenumber number
set colorcolumn=80

set nowrap
set shiftwidth=2
set tabstop=2
set expandtab
set incsearch
set hlsearch
set mouse=a
set wildmenu
set belloff=all
set list listchars=tab:·\ ,nbsp:•,trail:•
set showcmd
set noshowmode
set linespace=5
