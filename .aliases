# zshconfig
alias zshconfig="mvim ~/.zshrc"
alias ohmyzsh="mvim ~/.oh-my-zsh"

# Move terminal
alias l="ls -la"
alias ..="cd .."
alias sites="cd ~/Sites"
alias code="cd ~/Code"
alias restaurant="cd ~/Sites/restaurant"

# Mass Comunicacion
alias cliente="cd ~/Sites/masscliente"
alias admin="cd ~/Sites/massadmin"
alias reportes="cd ~/Sites/reportes"
alias aws="cd ~/.ssh && ssh -i ServerMassAWS.pem -v ubuntu@masscomunicacion.com.mx"

# Laravel
# export PATH=$HOME/bin:/usr/local/bin:~/.composer/vendor/bin:$PATH
alias t="vendor/bin/phpunit"
alias artisan="php artisan"
alias homestead="mvim ~/.homestead/Homestead.yaml"
function h() {
  (cd ~/Homestead && vagrant $*)
}

#Git
alias gs="git status"
alias gl="git log --oneline"
alias gcom="git checkout master"
alias gaa="git add -A"
alias gc="git commit -m "
alias gp="git push"
alias nah="git reset --hard && git clean -df"

