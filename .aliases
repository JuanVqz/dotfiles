# Projects aliases
alias aliases="nvim ~/.aliases"
alias dot="nvim ~/.dotfiles/config/nvim/init.vim"
alias notes="nvim ~/.notes.md"

alias code="cd ~/code"
alias me="cd ~/code/projects"
alias doctors="cd ~/code/projects/doctors"

alias ombu="cd ~/code/ombu/"
alias ncsa="cd ~/code/ombu/ncsa"
alias cpo="cd ~/code/ombu/c3po"

# Linux aliases
alias install="sudo apt-get install"
alias update="sudo apt-get update -y"
alias upgrade="sudo apt-get upgrade -y"
alias at="npx alacritty-themes"
alias bat="batcat"
alias c="clear"
alias r="ranger"
alias v="nvim"

# Git aliases
alias nah="git reset --hard && git clean -df"

